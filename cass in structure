#include <iostream>
using namespace std;

class point
{   
    //note here x & y are private because of class property
    // make it public otherwise it will throw compiler error , using public keyword
    public:
    int x ;
    int y;
};

int main()
{
    point p;
    p.x=10;
    p.y=20;
    
    cout << p.x <<" "
         <<p.y;
     
    return 0;    
}

// output : 10 20
