// sturcture in C
#include<stdio.h>
struct point 
{
    int x;
    int y;
    
};
int main()
{
struct point p ={10,20};
printf("%d %d", p.x,p.y);
return 0;

}
// Output : 10 20


// Structure in c++
#include <iostream>
using namespace std;

struct point 
{
    int x;
    int y;
    
};

int main()
{
    
struct point p ={10,20};
cout << p.x<<" "
     <<p.y;
     
return 0;

}
